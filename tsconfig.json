{
    "compilerOptions": {
        "target": "esnext",
        "sourceMap": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "strict": true,
        "esModuleInterop": true,
        "outDir": "./dist",
        
    },
    "include": [
        "./api/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}